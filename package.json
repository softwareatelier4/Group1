{
  "name": "jobadvisor",
  "version": "0.0.0",
  "description": "JobAdvisor - Software Engineering Atelier 2017",
  "main": "app.js",
  "scripts": {
    "install": "chmod -R u+x ./bin/ && ./bin/installSeleniumAndChromeWebDriver",
    "install-jenkins": "chmod -R u+x ./bin/ && ./bin/installSeleniumAndChromeWebDriverJenkins",
    "start": "DEBUG='jobadvisor' ./bin/www",
    "start:dev": "DEBUG='jobadvisor' nodemon ./bin/www -e html,css,dust,js,json,sh",
    "test": "sh test_local.sh",
    "test-fast": "sh test_local_no_download.sh",
    "test-mocha": "JUNIT_REPORT_PATH=./jenkins-test-results.xml istanbul cover --report cobertura _mocha -- --timeout 3000 -R spec ./test/* --recursive --colors --reporter mocha-jenkins-reporter",
    "test-nightwatch": "./nightwatch",
    "test-nightwatch-jenkins": "xvfb-run -a -e /dev/stdout ./nightwatch"
  },
  "author": "Group 1",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^0.3.1",
    "adaro": "~1.0.0",
    "bluebird": "~3.5.0",
    "body-parser": "^1.17.1",
    "debug": "^2.6.3",
    "dustjs-helpers": "~1.7.3",
    "dustjs-linkedin": "~2.7.2",
    "express": "^4.15.2",
    "method-override": "~2.3.5",
    "mocha-jenkins-reporter": "~0.3.6",
    "mongodb": "~2.2.22",
    "mongoose": "^4.9.0",
    "morgan": "^1.8.1",
    "nodemailer": "^4.0.1",
    "supertest": "^1.2.0"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "nightwatch": "~0.8.6",
    "should": "^4.0.4",
    "supertest": "~2.0.0"
  }
}
